		"copy-webpack-plugin": "^10.2.4",
		"cross-env": "^7.0.3",
		"css-loader": "^6.7.1",
		"file-loader": "^6.2.0",
		"jest-cli": "^28.1.0",
		"jest-svg-transformer": "^1.0.0",
		"jest-transform-stub": "^2.0.0",
		"style-loader": "^3.3.1",
		"svg-inline-loader": "^0.8.2",
		"webpack-cli": "^4.8.0"
	}
}
